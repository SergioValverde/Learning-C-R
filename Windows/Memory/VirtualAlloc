
FALLA

#include <stdio.h>
#include <stdlib.h>
#include <windows.h>
#include <stddef.h>


int main(void)
{
	int* nbytes = 1000;

	
	LPVOID p = VirtualAlloc(NULL, nbytes, MEM_COMMIT | MEM_RESERVE, PAGE_READWRITE);

	if (p = NULL) {
		
		printf("Valor Null"); // Si VirtualAlloc falla devolvera Null, check Return value froms docs.
		
	}
	else {
		printf(" |--- [+] RWX memory allocated: %p\n", p);
		strcpy((int*)p, nbytes);
		printf("Contenido de la memoria asignada: %d\n", (int*)p);
		VirtualFree(p, 0, MEM_RELEASE);

	}
	
	return 0;
}
